    <PackageReference Include="Rhino3dm" Version="7.15.0" />
  </ItemGroup>
  
  <ItemGroup>
    <ProjectReference Include="..\EZRX_UI\EZRX_UI.csproj" />
  </ItemGroup>
  
  <ItemGroup>
    <Reference Include="Ezrx.Rhino">
      <HintPath>obj\Debug\net48\Ezrx.Rhino.dll</HintPath>
    </Reference>
    <Reference Include="System.Windows.Forms" />
  </ItemGroup>

</Project>